{
  "expo": {
    "name": "Vaniyambadi Ride",
    "slug": "vanito",
    "version": "1.0.0",
    "orientation": "portrait",
    "scheme": "vaniyambadi",
    "userInterfaceStyle": "automatic",
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.vaniyambadi.ride"
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#ffffff"
      },
      "package": "com.vaniyambadi.ride",
      "googleServicesFile": "./android/app/google-services.json",
      "useNextNotificationsApi": true,
      "permissions": [
        "ACCESS_FINE_LOCATION",
        "ACCESS_COARSE_LOCATION",
        "INTERNET",
        "WAKE_LOCK",
        "RECEIVE_BOOT_COMPLETED",
        "FOREGROUND_SERVICE",
        "VIBRATE"
      ]
    },
    "plugins": [
      [
        "expo-notifications",
        {
          "icon": "./assets/notification-icon.png",
          "color": "#ffffff",
          "googleServicesFile": "./android/app/google-services.json"
        }
      ],
      "expo-router",
      "expo-font"
    ],
    "experiments": {
      "typedRoutes": true,
      "tsconfigPaths": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "37cfc536-fa90-45a9-9ac6-26ed08ff2646"
      }
    },
    "owner": "prince201"
  }
}
